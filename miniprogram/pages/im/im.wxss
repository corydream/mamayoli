page {
  background: #f6f6f6;
  display: flex;
  font-family: PingFangSC-Medium, PingFang SC;
}

.siwper {
  display: flex;
  height: 1000rpx;
  width: calc(100% - 64rpx);
  justify-content: center;
}

.swipper-item {
  flex-direction: row;
  padding-left: 16rpx;
}

.title {
  padding-top: 32rpx;
  padding-bottom: 24rpx;
  font-size: 120%;
}

.row {
  height: 100rpx;
  padding-top: 10rpx;
  display: flex;
  align-items: center;
}

.lable {
  display: inline-block;
  width: 160rpx;
  margin-right: 30rpx;
}

.content {
  display: inline-block;
}

/*中间的过度的横线*/

.line {
  width: 100%;
  height: 1px;
  border-top: solid #acc0d8 1px;
}

.addpic {
  height: 10rpx;
  width: 100%;
  display: block;
}
